package day12;

public class Ex{

            
            public static void main(String[] args) {
            System.out.println("FOR -----------------------");
            for (int i = 1; i <= 5; i++) {
                System.out.println(i);
            }
            System.out.println("BAD FOR, DONT USE LIKE THIS");
            int k = 1;
            for (; k <= 5; ) {
                System.out.println(k);
                k++;
            }

       }